<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>PasswordProtectedChecker.snk</AssemblyOriginatorKeyFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.3.8</Version>
    <PackageReleaseNotes>- Upgrade code to .NET Standard 2.0</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <PackageProjectUrl>https://github.com/Sicos1977/PasswordProtectedChecker</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Sicos1977/PasswordProtectedChecker</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>password zip outlook msg eml powerpoint word excel</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MsgReader" Version="4.3.1" />
    <PackageReference Include="OpenMcdf" Version="2.2.1.9" />
    <PackageReference Include="SharpZipLib" Version="1.3.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
